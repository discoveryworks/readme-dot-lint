  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#a09b92"
        }
      },
      {
        "name": "String",
        "scope": ["string", "punctuation.definition.string"],
        "settings": {
          "foreground": "#a26e2a"
        }
      },
      {
        "name": "Number",
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "#2e68dc"
        }
      },
      {
        "name": "Built-in constant",
        "scope": ["constant.language"],
        "settings": {
          "foreground": "#2e68dc"
        }
      },
      {
        "name": "User-defined constant",
        "scope": ["constant.character", "constant.other"],
        "settings": {
          "foreground": "#2e68dc"
        }
      },
      {
        "name": "Variable",
        "scope": ["variable"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Keyword",
        "scope": ["keyword"],
        "settings": {
          "foreground": "#780099"
        }
      },
      {
        "name": "Storage",
        "scope": ["storage"],
        "settings": {
          "foreground": "#780099"
        }
      },
      {
        "name": "Storage type",
        "scope": ["storage.type"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Class name",
        "scope": ["entity.name.class"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Function name",
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Function argument",
        "scope": ["variable.parameter"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#161fca"
        }
      },
      {
        "name": "Tag name",
        "scope": ["entity.name.tag"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Tag attribute",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#780099"
        }
      },
      {
        "name": "Library function",
        "scope": ["support.function"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Library constant",
        "scope": ["support.constant"],
        "settings": {
          "foreground": "#2e68dc"
        }
      },
      {
        "name": "Library class/type",
        "scope": ["support.type", "support.class"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Library variable",
        "scope": ["support.other.variable"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Invalid",
        "scope": ["invalid"],
        "settings": {
          "background": "#2e68dc",
          "foreground": "#ede2fd"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": ["invalid.deprecated"],
        "settings": {
          "background": "#161fca",
          "foreground": "#ede2fd"
        }
      },
      {
        "name": "JSON String",
        "scope": ["meta.structure.dictionary.json string.quoted.double.json"],
        "settings": {
          "foreground": "#4700b3"
        }
      },
      {
        "name": "diff.header",
        "scope": ["meta.diff", "meta.diff.header"],
        "settings": {
          "foreground": "#a09b92"
        }
      },
      {
        "name": "diff.deleted",
        "scope": ["markup.deleted"],
        "settings": {
          "foreground": "#2e68dc"
        }
      },
      {
        "name": "diff.inserted",
        "scope": ["markup.inserted"],
        "settings": {
          "foreground": "#780099"
        }
      },
      {
        "name": "diff.changed",
        "scope": ["markup.changed"],
        "settings": {
          "foreground": "#4700b3"
        }
      },
      {
        "name": "CSS Classes",
        "scope": ["entity.other.attribute-name.class.css"],
        "settings": {
          "foreground": "#4700b3"
        }
      },
      {
        "name": "CSS IDs",
        "scope": ["entity.other.attribute-name.id.css"],
        "settings": {
          "foreground": "#4700b3"
        }
      },
      {
        "name": "CSS Properties",
        "scope": ["support.type.property-name.css"],
        "settings": {
          "foreground": "#c8d327"
        }
      },
      {
        "name": "CSS Property Values",
        "scope": [
          "support.constant.property-value.css",
          "constant.numeric.css"
        ],
        "settings": {
          "foreground": "#a26e2a"
        }
      },
      {
        "name": "Markdown Headings",
        "scope": ["markup.heading"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#c8d327"
        }
      },
      {
        "name": "Markdown Links",
        "scope": ["markup.underline.link"],
        "settings": {
          "foreground": "#a26e2a"
        }
      },
      {
        "name": "Markdown Bold",
        "scope": ["markup.bold"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#161fca"
        }
      },
      {
        "name": "Markdown Italic",
        "scope": ["markup.italic"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#161fca"
        }
      }
    ]
  },
